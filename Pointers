#include <iostream>
using namespace std;

int main() {
    int n=5,v;
    cout << n << endl;
    cout << &n << endl;
    int* ptr = &n;
    cout << ptr <<endl;
    cout << *ptr <<endl;
    int* ptr2 = &v;
    v = 7;
    cout << *ptr2 <<endl;
    cout << ptr2 <<endl;
    cout << v <<endl;
    cout << &v <<endl;
}
